    "awesome-bootstrap-checkbox": "https://github.com/flatlogic/awesome-bootstrap-checkbox.git#~0.3.5",
    "bootstrap-datepicker": "https://github.com/eternicode/bootstrap-datepicker.git#~1.5.0",
    "js-cookie": "~2.0.4",
    "datatables.net-bs": "~1.10.10",
    "tinymce": "~4.3.2",
    "perfect-scrollbar": "~0.6.8"
